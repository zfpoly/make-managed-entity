<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

        <services>
            <defaults public="false" />

            <service id="maker.maker.make_managed_entity" class="Symfony\Bundle\MakerBundle\Maker\MakeManagedEntity">
                <argument type="service" id="maker.doctrine_helper_extension" />
                <argument type="service" id="maker.file_manager_extension" />
                <argument type="service" id="maker.generator_extension" />
                <tag name="maker.command" />
            </service>
        </services>
</container>
